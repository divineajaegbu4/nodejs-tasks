<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Users</title>
  </head>
  <body>
    <h1>Users</h1>

    <% users.forEach(user => { %>
    <p>FirstName: <%= user.firstName %>!</p>
    <p>LastName: <%= user.lastName %>!</p>
    <p>Email: <%= user.email %>!</p>
    <p>Role: <%= user.role %>!</p>
    -----------------------------------
    <%}) %>
  </body>
</html>
